<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style android:name="Circular">
        <item android:name="cornerSize">50%</item>
    </style>

    <style android:name="RoundedSquare">
        <item android:name="cornerSize">10%</item>
    </style>

    <style android:name="CornerCut">
        <item android:name="cornerSize">15dp</item>
